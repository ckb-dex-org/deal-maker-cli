{
  "name": "deal-maker-cli",
  "version": "0.1.0",
  "description": "",
  "bin": "lib/cli/index.js",
  "engines": {
    "node": ">=12.9.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node src/cli run",
    "start:config": "cross-env NODE_ENV=development ts-node src/cli config",
    "start:watch": "nodemon",
    "start:cli": "ts-node src/cli",
    "start:prod": "cross-env NODE_ENV=production node lib/cli",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "build": "cross-env NODE_ENV=production yarn run tsc",
    "clean": "npx rimraf lib && npx rimraf tsconfig.tsbuildinfo",
    "format": "prettier -w --ignore-unknown src/**/*",
    "db:init": "typeorm migration:generate -c production -n CreateDatabase -d src/migrations"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:glias/deal-maker-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "cross-env": "7.0.2",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "lint-staged": "10.4.0",
    "nodemon": "2.0.5",
    "prettier": "2.1.2",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "class-validator": "^0.12.2",
    "commander": "6.1.0",
    "inversify": "5.0.1",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.0.0",
    "tslib": "2.0.3",
    "typeorm": "0.2.28",
    "winston": "3.3.3"
  }
}
